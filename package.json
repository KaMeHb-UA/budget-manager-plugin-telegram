{
	"name": "nothingtechwallet-wallet-service",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"prebuild": "rimraf dist",
		"build": "nest build && node build && cd dist && rimraf src tsconfig.build.tsbuildinfo && yarn install --frozen-lockfile && rm yarn.lock package.json",
		"format": "prettier --write \"src/**/*.ts\"",
		"start": "nest start",
		"start:dev": "nest start --watch",
		"start:debug": "nest start --debug --watch",
		"start:prod": "set -o allexport && source ./.env && set +o allexport && cd dist && node main",
		"lint": "eslint \"src/**/*.ts\" --fix"
	},
	"dependencies": {
		"@nestjs/common": "^9.0.8",
		"@nestjs/config": "^2.2.0",
		"@nestjs/core": "^9.0.8",
		"@nestjs/microservices": "^9.0.8",
		"reflect-metadata": "^0.1.13",
		"rxjs": "^7.5.6"
	},
	"devDependencies": {
		"@nestjs/cli": "^9.0.0",
		"@types/node": "^18.0.0",
		"@typescript-eslint/eslint-plugin": "^5.0.0",
		"@typescript-eslint/parser": "^5.0.0",
		"@yarnpkg/lockfile": "^1.1.0",
		"dotenv": "^16.0.1",
		"esbuild": "^0.14.43",
		"eslint": "^8.0.1",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-prettier": "^4.0.0",
		"prettier": "^2.3.2",
		"rimraf": "^3.0.2",
		"source-map-support": "^0.5.20",
		"typescript": "^4.3.5"
	}
}
